<mvc:View
    controllerName="com.tasa.registrodtolvas.controller.NotFound"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc">

    <MessagePage
        title="{i18n>objectTitle}"
        text="{i18n>noObjectFoundText}"
        icon="sap-icon://alert"
        description=""
        id="page"
        showNavButton="true"
        navButtonPress="onNavBack">
        <customDescription>
            <Link id="link" text="{i18n>backToWorklist}" press=".onLinkPressed" />
        </customDescription>
    </MessagePage>

</mvc:View>