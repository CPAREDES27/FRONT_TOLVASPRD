                                            </t:template>
                                        </t:Column>
                                        <t:Column
                                            width="auto">
                                            <Label text="Planta" textAlign="Left" width="100%" />
                                            <t:template>
                                                <Text text="{DescargaTolvaModel>DSPTA}"/>
                                            </t:template>
                                        </t:Column>
                                        <t:Column
                                            width="auto">
                                            <Label text="Especie" textAlign="Left" width="100%" />
                                            <t:template>
                                                <Text text="{DescargaTolvaModel>DSSPC}"/>
                                            </t:template>
                                        </t:Column>
                                        <t:Column
                                            width="auto">
                                            <Label visible="false" />
                                            <t:template>
                                                <Link text="Ejecutar" visible="{:= ${DescargaTolvaModel>CMIN} === 'E'}" emphasized="true" press="sap.m.MessageBox.alert('hiciste click')"/>
                                            </t:template>
                                        </t:Column>
                                    </t:columns>
                                </t:Table>
                                <!-- <core:ComponentContainer width="100%" id="myChildContainer"/> -->
                            </content>
                            <footer>
