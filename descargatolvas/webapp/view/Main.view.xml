                                            <t:template>
                                                <core:Icon src="{
                                                    path: 'DescargaTolvaModel>DOC_MIGO',
                                                    formatter: 'com.tasa.tolvas.descargatolvas.controller.formatter.showCheckedIcon'
                                                }">
                                                </core:Icon> 
                                            </t:template>
                                        </t:Column>
                                        <t:Column
                                            width="auto">
                                            <Label text="Fecha producción" textAlign="Left" wrapping="true" width="100%" />
                                                <t:template>
                                                    <Text text="{
                                                        path: 'DescargaTolvaModel>FECCONMOV',
                                                        type: 'sap.ui.model.type.Date',
                                                        formatOptions: {
                                                            pattern: 'dd/MM/yyyy',
                                                            source: {
                                                                pattern: 'yyyyMMdd'
                                                            }
                                                        }
                                                    }"/>
                                            </t:template>
                                        </t:Column>
                                        <t:Column
                                            width="auto">
                                            <Label text="Peso modificado" textAlign="Right" wrapping="true" width="100%" />
                                            <t:template>
                                                <Text text="{DescargaTolvaModel>PESACUMOD}" textAlign="Right"/>
                                            </t:template>
                                        </t:Column>
                                        <t:Column
                                            width="auto">
                                            <Label text="Nro. pedido" textAlign="Left" wrapping="true" width="100%"/>
                                            <t:template>
                                                <Text text="{DescargaTolvaModel>NROPEDI}"/>
                                            </t:template>
