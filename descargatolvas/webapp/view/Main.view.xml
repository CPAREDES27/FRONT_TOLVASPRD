<mvc:View
    controllerName="com.tasa.tolvas.descargatolvas.controller.Main"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:form="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:layout="sap.ui.layout"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:uxap="sap.uxap"
    xmlns:t="sap.ui.table"
    displayBlock="true" >
	<!-- <Shell id="shell">
		<App id="app"> -->
			<!-- <pages> -->
            <f:FlexibleColumnLayout
                id="fcl"
                layout="{/layout}"
                backgroundDesign="Translucent">
                <f:beginColumnPages>
                    
                        <Page id="page" title="{i18n>title}">
                            <content>
                                <form:Form width="100%" editable="true" visible="true" binding="{FormSearchModel>/SupplierCollection/0/}">
                                    <form:title>
                                        <core:Title text="Datos de selección" level="Auto"/>
                                    </form:title>
                                    <form:formContainers>
                                        <form:FormContainer visible="true">
                                            <form:formElements>
                                                <form:FormElement visible="true">
                                                    <form:label>
                                                        <Label text="Centro de descarga" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                                    </form:label>
                                                    <form:fields>
                                                        <Input value="{FormSearchModel>CentroDescargaBottom}" type="Text" showValueHelp="true" enabled="true" visible="true" width="auto" valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="0" valueHelpRequest="_onInputValueHelpRequest"/>
                                                        <Input value="{FormSearchModel>CentroDescargaTop}" type="Text" showValueHelp="true" enabled="true" visible="true" width="auto" valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="0" valueHelpRequest="_onInputValueHelpRequest"/>
                                                    </form:fields>
                                                </form:FormElement>
                                                <form:FormElement visible="true">
                                                    <form:label>
                                                        <Label text="Fecha inicio de descarga" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                                    </form:label>
                                                    <form:fields>
                                                        <DatePicker value="{FormSearchModel>FechaInicioDescargaBottom}" width="auto" displayFormat="medium" required="false" valueStateText="Invalid entry" enabled="true" visible="true" valueFormat="yyyyMMdd"/>
                                                        <DatePicker value="{FormSearchModel>FechaInicioDescargaTop}" width="auto" displayFormat="medium" required="false" valueStateText="Invalid entry" enabled="true" visible="true" valueFormat="yyyyMMdd"/>
                                                    </form:fields>
                                                </form:FormElement>
                                                <form:FormElement visible="true">
                                                    <form:label>
                                                        <Label text="Hora inicio de descarga" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                                    </form:label>
                                                    <form:fields>
                                                        <TimePicker value="{FormSearchModel>HoraInicioDescargaBottom}" valueFormat="HH:mm" displayFormat="HH:mm"/>
                                                        <TimePicker value="{FormSearchModel>HoraInicioDescargaTop}" valueFormat="HH:mm" displayFormat="HH:mm"/>
                                                    </form:fields>
                                                </form:FormElement>
                                                <form:FormElement visible="true">
                                                    <form:label>
                                                        <Label text="Fecha de producción" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                                    </form:label>
                                                    <form:fields>
                                                        <DatePicker value="{FormSearchModel>FechaProduccionBottom}" width="auto" displayFormat="medium" required="false" valueStateText="Invalid entry" enabled="true" visible="true" valueFormat="yyyyMMdd"/>
                                                        <DatePicker value="{FormSearchModel>FechaProduccionTop}" width="auto" displayFormat="medium" required="false" valueStateText="Invalid entry" enabled="true" visible="true" valueFormat="yyyyMMdd"/>
                                                    </form:fields>
                                                </form:FormElement>
                                                <form:FormElement visible="true">
                                                    <form:label>
                                                        <Label text="Matrícula de Embarcación" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                                    </form:label>
                                                    <form:fields>
                                                        <Input value="{FormSearchModel>MatriculaEmbarcacionBottom}" type="Text" showValueHelp="true" enabled="true" visible="true" width="auto" valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="0" valueHelpRequest="_onInputValueHelpRequest"/>
                                                        <Input value="{FormSearchModel>MatriculaEmbarcacionTop}" type="Text" showValueHelp="true" enabled="true" visible="true" width="auto" valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="0" valueHelpRequest="_onInputValueHelpRequest"/>
                                                    </form:fields>
                                                </form:FormElement>
                                                <form:FormElement visible="true">
                                                    <form:label>
                                                        <Label text="Clase de Mensaje" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                                    </form:label>
                                                    <form:fields>
                                                        <ComboBox selectedKey="{FormSearchModel>ClaseMensajeBottom}" items="{ClaseMensajeModel>/data}" editable="true" enabled="true" visible="true" width="auto" valueState="None" maxWidth="100%">
                                                            <items>
                                                                <core:Item key="{ClaseMensajeModel>id}" text="{ClaseMensajeModel>descripcion}"/>
                                                            </items>
                                                        </ComboBox>
                                                        <ComboBox selectedKey="{FormSearchModel>ClaseMensajeTop}" items="{ClaseMensajeModel>/data}" editable="true" enabled="true" visible="true" width="auto" valueState="None" maxWidth="100%">
                                                            <items>
                                                                <core:Item key="{ClaseMensajeModel>id}" text="{ClaseMensajeModel>descripcion}"/>
                                                            </items>
                                                        </ComboBox>
                                                    </form:fields>
                                                </form:FormElement>
                                                <form:FormElement visible="true">
                                                    <form:label>
                                                        <Label text="Cantidad de Filas" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                                    </form:label>
                                                    <form:fields>
                                                        <Input value="{
                                                                path: 'FormSearchModel>CantidadFilas',
                                                                type: 'sap.ui.model.type.Integer',
                                                                constraints: 
                                                                {
                                                                    minimum: 0
                                                                }
                                                            }" type="Number" enabled="true" visible="true" width="auto" valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="0">
                                                            <layoutData>
                                                                <!-- <FlexItemData growFactor="1" /> -->
                                                                <layout:GridData span="XL5 L5 M5 S12" />
                                                            </layoutData>
                                                        </Input>
                                                    </form:fields>
                                                </form:FormElement>
                                                <form:FormElement visible="true">
                                                    <form:label>
                                                        <Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                                    </form:label>
                                                    <form:fields>
                                                        <Button text="Buscar" type="Default" icon="sap-icon://search" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"
                                                            press="_onButtonSearchPress">
                                                            <layoutData>
                                                                <layout:GridData span="XL5 L5 M5 S12" />
                                                            </layoutData>
                                                        </Button>
                                                    </form:fields>
                                                </form:FormElement>
                                            </form:formElements>
                                            <form:title/>
                                        </form:FormContainer>
                                    </form:formContainers>
                                    <form:layout>
                                        <form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="2" columnsM="1" labelSpanM="2" labelSpanS="12"/>
                                    </form:layout>
